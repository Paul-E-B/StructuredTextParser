<Window x:Class="StructuredTextParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StructuredTextParser"
        mc:Ignorable="d"
        Title="Structured Text Parser" Height="222" Width="222">
    <Grid>
        <Button x:Name="TheButton" Content="Active" HorizontalAlignment="Center" Margin="0,17,0,0" VerticalAlignment="Top" Click="Button_Click" RenderTransformOrigin="-1.136,0.553" Height="111" Width="111" BorderBrush="Black" FontSize="30" Background="Red" Foreground="White" BorderThickness="5,5,5,5"/>
        <TextBox x:Name="TheText" HorizontalAlignment="Center" Margin="0,166,0,0" TextWrapping="Wrap" Text="Place files in &quot;resources&quot; folder..." VerticalAlignment="Top" Width="202"/>
    </Grid>
</Window>
